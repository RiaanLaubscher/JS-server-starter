{
  "name": "baba-spinasie-server",
  "version": "1.0.0",
  "description": "This repo contains the server side project for the baba-spinasie web-app",
  "main": "app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "tsnd --respawn app.ts",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:all:undo": "sequelize-cli db:seed:undo:all",
    "seed:undo": "sequelize-cli db:seed:undo",
    "db:drop": "sequelize-cli db:drop",
    "db:create": "sequelize-cli db:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RiaanLaubscher/baba-spinasie-server.git"
  },
  "keywords": [],
